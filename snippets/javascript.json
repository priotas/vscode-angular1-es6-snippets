{
	"Angular1 Component": {
		"prefix": "acom",
		"body": [
			"import controller from './${name}.controller';",
			"import template from './${name}.html';",
			"",
			"let component = {",
			"    controller,",
			"    template,",
			"    bindings: {}",
			"};",
			"",
			"export default component;"
		],
		"description": "Angular 1 component â˜ "
	},

	"Angular1 Component Controller": {
		"prefix": "acon",
		"body": [
			"export default class ${name}Controller {",
			"",
			"    constructor() {",
			"        'ngInject';",
			"    }",
			"",
			"    $$onInit() {",
			"",
			"    }",
			"",
			"    $$onChanges(changesObj) {",
			"",
			"    }",
			"",
			"    $$doCheck() {",
			"",
			"    }",
			"",
			"    $$onDestroy() {",
			"",
			"    }",
			"",
			"    $$postLink() {",
			"",
			"    }",
			"",
			"}"
		],
		"description": "Angular 1 component controller ðŸ˜®"
	},

	"Angular1 Module": {
		"prefix": "amod",
		"body": [
			"import angular from 'angular';",
			"import uiRouter from 'angular-ui-router';",
			"",
			"const MODULE_NAME = '${name}';",
			"",
			"angular.module(MODULE_NAME, [",
			"    uiRouter",
			"]);",
			"",
			"export default MODULE_NAME;"
		],
		"description": "Angular 1 module ðŸ˜´"
	},

	"Angular1 Factory": {
		"prefix": "afac",
		"body": [
			"let ${name}Factory = function () {",
			"  const ${value} = {};",
			"",
			"  let get${Value} = () => {",
			"    return ${value};",
			"  };",
			"",
			"  return { get${Value}  };",
			"};",
			"",
			"export default ${name}Factory;"
		],
		"description": "Angular 1 factory ðŸ˜±"
	}
}
